      <Button layoutX="752.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="51.0" prefWidth="48.0" style="-fx-background-color: transparent;" />
      <Button layoutX="704.0" layoutY="6.0" mnemonicParsing="false" onAction="#openMainMenu" prefHeight="51.0" prefWidth="48.0" style="-fx-background-color: transparent;" />
