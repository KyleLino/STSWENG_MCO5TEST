      <Label fx:id="labelEaten" alignment="TOP_CENTER" contentDisplay="CENTER" layoutX="610.0" layoutY="287.0" prefHeight="17.0" prefWidth="68.0" text="0" textAlignment="RIGHT" textFill="WHITE">
